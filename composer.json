{
    "name": "codecov/codecov-php",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/codecov/codecov-php",
    "description": "demo repo",
    "keywords": ["code coverage", "codecov", "coverage reports"],
    "authors": [
        {
            "name": "codecov.io",
            "email": "trent@codecov.io"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5.13"
    },
    "autoload": {
        "psr-0": {
            "Example": "src/"
        }
    },
    "scripts": {
        "coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-clover coverage.xml"
    }
}
